{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "declaration": true,
        "removeComments": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "src/ts-framework-build/"
    },
    "exclude": [
        "src/ts-framework-build",
        "node_modules",
        "typings",
        "typings/main",
        "typings/main.d.ts",
        "typings/index.d.ts",
        "typings/browser",
        "typings/browser.d.ts"
    ]
}